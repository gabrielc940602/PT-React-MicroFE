{
  "name": "portals-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "rimraf dist  && concurrently  \"pnpm builddev:server\"  \"pnpm builddev:client\" \"ping 127.0.0.1 -n 8 > nul  && browser-refresh .\\dist\\server\\main.js\"",
    "start": "pnpm build && pnpm serve",
    "serve": "node dist/server/main.js",
    "builddev:client": "webpack --mode development --watch --progress  --config webpack/webpack.client.js",
    "builddev:server": "webpack --mode development --watch  --progress --config webpack/webpack.server.js",
    "build": "rimraf dist && pnpm build:server && pnpm build:client",
    "build:client": "webpack --mode production --progress --config webpack/webpack.client.js",
    "build:server": "webpack --mode production --progress --config webpack/webpack.server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.9.0",
    "@everapi/freecurrencyapi-js": "^1.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@module-federation/typescript": "^3.1.1",
    "axios": "^1.6.7",
    "browser-refresh": "^1.7.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.1",
    "dotenv-webpack": "^8.0.1",
    "express": "4.18.2",
    "fast-xml-parser": "^4.3.4",
    "file-loader": "^6.2.0",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-middleware": "^3.5.0",
    "mini-css-extract-plugin": "^2.8.0",
    "primeicons": "^6.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.22.0",
    "regenerator-runtime": "0.14.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "scss-loader": "^0.0.1",
    "style-loader": "^3.3.4",
    "tslib": "^2.6.2",
    "url-loader": "^4.1.1",
    "yup": "^1.3.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/node": "7.22.19",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@module-federation/node": "0.16.2",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.14",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "babel-loader": "8.2.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.2.5",
    "rimraf": "3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.10.0"
  }
}
